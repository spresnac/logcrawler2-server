{
    "name": "spresnac/logcrawler-server-new",
    "type": "project",
    "description": "A laravel software to combine all your laravel project's logs into one place",
    "keywords": ["laravel", "logging", "on premise", "log", "collect"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fruitcake/laravel-cors": "2.0.5",
        "guzzlehttp/guzzle": "7.4.1",
        "laravel/framework": "8.82.0",
        "laravel/jetstream": "2.6.4",
        "laravel/sanctum": "2.14.0",
        "laravel/tinker": "2.7.0",
        "livewire/livewire": "2.10.1",
        "spresnac/laravel-create-user-cli": "1.0.5",
        "spresnac/logcrawler-client": "2.0.0-beta.1",
        "spresnac/laravel-artisan-database-helper": "1.0.9"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.6.6",
        "beyondcode/laravel-dump-server": "1.7.0",
        "facade/ignition": "2.17.4",
        "fakerphp/faker": "1.18.0",
        "laravel/sail": "1.13.1",
        "mockery/mockery": "1.5.0",
        "nunomaduro/collision": "5.11.0",
        "nunomaduro/larastan": "1.0.3",
        "phpunit/phpunit": "9.5.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
